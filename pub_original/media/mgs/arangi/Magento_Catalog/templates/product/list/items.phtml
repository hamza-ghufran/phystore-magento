													<span class="text-add-tocart"><?php /* @escapeNotVerified */ echo __('+ Add to Cart') ?></span>
                                                    <i class="icon-shopping-cart1 mini-cart"></i>
												</button>
											<?php endif; ?>
										<?php else: ?>
											<?php if ($_item->isAvailable()): ?>
												<div class="stock available"><span><?= /* @escapeNotVerified */ __('In stock') ?></span></div>
											<?php else: ?>
												<div class="stock unavailable"><span><?= /* @escapeNotVerified */ __('Out of stock') ?></span></div>
											<?php endif; ?>
										<?php endif; ?>
									<?php endif; ?>
